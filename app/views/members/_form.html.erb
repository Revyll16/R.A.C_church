<%= form_with model: @member do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :phone %>
    <%= f.telephone_field :phone %>
  </div>
  <div>
    <%= f.label :role %>
    <%= f.text_field :role %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
